	<script>
		$(function() {
			/*
			$('#fullpage').fullpage({
				scrollBar: true,
				autoScrolling: false,
				fitToSection: false,
				recordHistory:false
			});
			*/

			// dialog : video player
			$('.btn-play').on('click', function(e) {
				var url = $(this).data('url') + '?autoplay=1&showinfo=0&fs=0&rel=0&wmode=transparent',
					hash = $(this).data('name'),
					$wrap = $('<div><button type="button" class="btn-close"><span>close</span></button></div>').addClass('popup').appendTo('body'),
					scrollTop = $(window).scrollTop();

				$('html').addClass('show-popup');

				$wrap.append('<iframe width="560" height="315" src="" frameborder="0" allowfullscreen></iframe>')
					.find('iframe').attr({src:url});

				$wrap.on('click', '.btn-close', function() {
					$(this).closest('.popup').remove();
					$('html').removeClass('show-popup');

					$(window).scrollTop(scrollTop);
				});

				window.location.hash = hash;

				e.preventDefault();
			});

			// dialog : image viewer
			$('.btn-view').on('click', function(e) {
				var $viewer = $('.viewer'),
					$index = $('.slider-gallery .slick-slide:not(.slick-cloned) .item').index($(this).closest('.item'));
				$('html').addClass('show-popup');

				$viewer.addClass('active')
					.find('.btn-close').on('click', function() {
						$viewer.removeClass('active');
					});

				$('.slider-for:not(.slick-slider)').slick({
					arrows: true,
					dots: false,
					lazyLoad: 'ondemand',
					initialSlide:$index
				});
				$('.slider-for.slick-slider').slick('slickGoTo', $index, true);
			});

			//tab
			$.fn.tab = function(option){
				var $this = $(this),
					$tab = $this.find(option.tab),
					$contents = $this.find(option.cnt),
					$index = option.random === true ? Math.floor(Math.random() * $tab.length) : 0;

				function showContent() {
					$tab.eq($index).addClass(option.current)
						.siblings($tab.selector).removeClass(option.current);

					$contents
						.eq($index).show()
						.siblings($contents.selector).hide();

					if(option.callback) option.callback();
				};

				$tab.on('click', function(e) {
					e.preventDefault();
					var $i = $(this).index($tab.selector);
					$index = $i;
					showContent();
					return false;
				});

				showContent();
			};

			$('.multimedia').tab({
				tab : '.nav-tabs > a ' , // 클릭하는 태그
				cnt : '.tab-contents', //  컨텐츠
				current : 'active', // 활성화 class
				callback: function() {
					$('.slider-gallery:visible:not(.slick-slider)').slick({
						arrows: false,
						lazyLoad: 'ondemand',
						dots: true
					});
				}
			});

			// slider
			$('.slider-film').slick({
				arrows: false,
				dots : true
			});
		});
	</script>

