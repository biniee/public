@charset "utf-8";

@font-face {
    font-family: 'kiaB';
    font-weight: normal;
    font-style: normal;
    src: url('../fonts/kia_bold.eot');
    src: url('../fonts/kia_bold.eot?#iefix') format('embedded-opentype'),url('../fonts/kia_bold.woff') format('woff')
}
@font-face {
    font-family: 'kiaM';
    font-weight: normal;
    font-style: normal;
    src: url('../fonts/kia_medium.eot');
    src: url('../fonts/kia_medium.eot?#iefix') format('embedded-opentype'),url('../fonts/kia_medium.woff') format('woff')
}


/*reset*/
html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, embed,
figure, figcaption, footer, header, hgroup,
menu, nav, output, ruby, section, summary,
time, mark, audio, video {margin:0; padding:0; border:0; font:inherit; vertical-align:baseline; font-weight:normal;}
address,em {font-style:normal; font-weight:normal;}
strong{font-weight:bold}
input,button,select {font-family:inherit; margin:0; padding:0; vertical-align:middle; }
article, aside, details, figcaption, figure,
footer, header, hgroup, menu, nav, section, main{display:block;}
ol,ul,li {list-style:none;}
button{margin:0; padding:0; border:0; background-color:transparent; -webkit-appearance:button; cursor:pointer;}
body{font-family:KiaM; }
a{text-decoration:none; color:#666;}
a:hover {}
button:focus,
input:focus,
a:focus {outline:thin dotted; outline:none; outline-offset:-2px;}
[tabindex="-1"]:focus {outline:none;}
html{overflow-y:scroll !important;}
.none{display:none;}

/* z-index

header:10
footer:10
dim : 99
popup : 100
layer : 200
layer(main) : 10

*/

html {height:100%; overflow-y:scroll;}
.wrap{position:relative; height:100%;}
#header{position:absolute; top:-80px; height:80px; display:block; width:100%; min-width:1000px; text-align:center; box-sizing:border-box; }
#footer{height:90px;}
main #header,
.fp-enabled #header {top:0;}
body {min-width:1000px; height:100%; min-height:900px; padding-top:80px; padding-bottom:90px; -webkit-box-sizing:border-box; box-sizing:border-box; color:#fff; font-size:14px;}
.fp-enabled body {min-height:auto; padding-top:0; padding-bottom:0;}
.fp-enabled .wrap {height:auto;}
.fp-enabled #footer {position:absolute; right:0; bottom:0; left:0; height:auto;}
main,
#fullpage,
#fullpage .section {height:100%;}

/* common */
header{position:absolute; left:0; right:0; top:0; width:100%; height:80px; z-index:10; background-color:#0a0b0d;}
main header{background-color:transparent;}
header div{position:relative; min-width:1000px; max-width:1440px; margin:0 auto;}
header div:after{content:''; display:block; clear:both; }
header h1{position:absolute; left:30px; top:34px;}
header nav{width:100%; height:80px; }
header nav ul{text-align:center;}
header nav li{display:inline-block; padding:0 15px;}
header nav li a{display:table-cell; height:80px; font:0/0 a; vertical-align:middle; }
header nav li a:after{content:''; display:inline-block; height:12px; background:url('../images/gnb.png')}
header nav li.whos a:after{width:111px; background-position:0 0;}
header nav li.features a:after{width:77px; background-position:0 -25px;}
header nav li.vr a:after{width:49px; background-position:0 -50px;}
header nav li.media a:after{width:94px; background-position:0 -75px;}
header nav li.brand a:after{width:107px; background-position:0 -100px;}
header nav li.whos.active a:after{background-position:-150px 0;}
header nav li.features.active a:after{background-position:-150px -25px;}
header nav li.vr.active a:after{background-position:-150px -50px;}
header nav li.media.active a:after{background-position:-150px -75px;}
header nav li.brand.active a:after{background-position:-150px -100px;}
header .btn-calc{position:absolute; right:30px; top:12px}

footer{}
footer:before{content:''; position:absolute; left:0; right:0; height:90px; display:block; background-color:#0a0a0a;}
footer > div {position:relative; z-index:10; max-width:1200px; min-width:1000px; height:90px; margin:0 auto; font:0/0 a; overflow:hidden;}
footer > div .logo{position:absolute; top:24px; width:285px; height:46px; background:url('../images/txt_footer.png') no-repeat 0 0;}
footer > div .logo h1{ width:70px; height:46px;}
footer > div .logo a{display:block; width:100%; height:100%;}
footer > div .menu-local{position:absolute; top:20px; left:378px; background:url('../images/txt_footer.png') no-repeat 0 -50px;}
footer > div .menu-local li{float:left; width:38px; height:12px;}
footer > div .menu-local li:last-child{width:60px;}
footer > div .menu-local li a{display:block; width:100%; height:100%;}
footer > div dl{position:absolute; left:378px; top:45px; width:100%; height:25px; background:url('../images/txt_footer.png') no-repeat 0 -75px;}
footer > div .menu-social{position:absolute; right:0; top:20px; background:url('../images/txt_footer.png') no-repeat 10px -100px;}
footer > div .menu-social li{float:left; width:18px; height:18px; margin-left:10px}
footer > div .menu-social li a{display:block; width:100%; height:100%;}


.wrap-video{position:relative;}
.wrap-video iframe{width:100%; height:100%;}
.wrap-video > img{position:absolute; z-index:10; left:0; right:0; top:0; bottom:0; width:100%; }
.btn-play{position:absolute; left:0; top:0; right:0; bottom:0; display:block; width:100%; height:100%; cursor:pointer; font:0/0 a; }
.btn-play:after{content:''; position:absolute; left:50%; top:50%; z-index:10; display:block; width:76px; height:76px; margin:-38px 0 0 -38px; background:url('../images/btn_play.png')}
.btn-play.small:after{width:60px; height:60px; margin:-30px 0 0 -30px; background:url('../images/btn_play_s.png') }


/* popup */
.popup.active + .dim,
.layer.active + .dim{position:fixed; left:0; right:0; top:0; bottom:0; z-index:99; background-color:rgba(0,0,0,0.6); }
.layer.active {display:block;}
.show-popup,
.show-popup body {overflow:hidden;}
.popup {position:fixed; top:0; right:0; bottom:0; left:0; z-index:100; background-color:#000;}
.popup iframe {width:100%; height:100%;}
.popup .btn-close,
.popup.layer .btn-close{position:absolute; right:0; top:0; width:46px; height:46px; font:0/0 a; background:url('../images/btn_close.png') no-repeat center center}

/* popup layer : full */
.popup.layer.full{position:absolute; left:50%; right:inherit; top:25%; bottom:inherit; width:1000px; height:689px; margin-left:-500px; background-color:#fff;}
.popup.layer.full .contents{padding:0 74px 74px; color:#101010; text-align:center;}
.popup.layer.full .contents h2{font-size:38px; line-height:134px; letter-spacing:15px;}
.popup.layer.full .contents .wrap-video{width:852px; height:480px;}

.indicator:after{content:''; display:inline-block; height:100%; vertical-align:middle;}
.indicator ol{display:inline-block; vertical-align:middle; }
.indicator li{width:10px; height:10px; margin-top:15px; background:url('../images/bg_indicator.png') no-repeat 0 0;}
.indicator li.active{background-position:0 -10px;}
.indicator a{display:block; width:10px; height:10px; font-size:0;}



/* main */
/*.main{background:#07080a url('../images/bg_main.jpg') no-repeat 50% 50%; background-size:contain }*/
main {}
.main-page {padding-top:0;}
.main .inner {position:relative; max-width:1000px; margin:0 auto;}
.main{background:#07080a url('../images/bg_main.png') no-repeat center bottom;}
.main > div{position:relative; min-width:1000px; height:100%; margin:0 auto; overflow:hidden;}


.main .box-title{padding-top:250px; text-align:center;}
.small-page .main .box-title {padding-top:200px;}
.main .box-title h2{margin:0 auto 20px;}

.main .box-car{text-align:center;}

.main .box-layer{}
.main .box-layer > [class*="btn"]{position:absolute; top:67%; height:140px;}

.main .box-layer .btn-whos{left:0; }
.main .box-layer .btn-feature{right:0;}
.main .box-layer .btn-launching{left:0; z-index:2; display:block; margin-top:-215px;}

.main .box-layer .border .before,
.main .box-layer .border .after{content:''; position:absolute; left:-4px; z-index:1; display:block; width:178px; height:30px; }
.main .box-layer .border .before{top:-10px; background:url('../images/btn_main_top.png')}
.main .box-layer .border .after{bottom:0px; background:url('../images/btn_main_bottom.png')}

.main .border{display:inline-block; position:relative; width:170px; height:140px; text-align:center;}
.main .border .light{position:absolute; top:-4px; left:0; z-index:2;-webkit-filter: blur(3px);  -moz-filter: blur(3px); filter: blur(3px); }
.main .border .txt{display:inline-block; margin-top:10px;}
.main .border .img{display:inline-block;}

.main .box-layer .layer{display:none; position:absolute; bottom:6%; z-index:10; width:790px; height:490px; padding:0; font:0/0 a;}
.main .box-layer .layer.active{display:block;}
.main .box-layer .layer h3{font:0/0 a;}
.main .box-layer.whos .layer{left:0; background:url('../images/bg_main_layer01.png')}
.main .box-layer.feature .layer{right:0; background:url('../images/bg_main_layer02.png')}
.main .box-layer.whos .layer a{position:absolute; right:60px; bottom:75px; z-index:10; display:block; width:242px; height:40px; font:0/0 a; background-image:url('data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7');}
.main .box-layer.feature .layer a{position:absolute; left:60px; bottom:75px; z-index:10; display:block; width:242px; height:40px; font:0/0 a; background-image:url('data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7');}

.layer-launching{width:861px; height:570px; top:50%; left:50%; margin-top:-285px; margin-left:-430px; box-sizing:border-box;}
.layer.layer-launching .box-title{height:31px; font:0/0 a; border-bottom:none; background:url('../images/tit_launching.png') no-repeat 0 0;}
.layer.layer-launching  h3{font:0/0 a;}
.layer-launching .wrap-video{height:440px;}



/* common */
.section > div{position:relative; height:100%;}
.section > div.fp-controlArrow.fp-prev,
.section > div.fp-controlArrow.fp-next,
.section > div.fp-slidesNav{position:absolute;}

.box-title{position:relative; z-index:2; width:1000px; margin:0 auto; text-align:left;}
.box-title h2{margin-bottom:20px;}
.box-title p{padding-left:20px;}

/* multimedia : common */

.box-list{padding:60px 0 80px; text-align:center;}
.box-list ul{max-width:1240px; margin:0 auto; text-align:left;}
.box-list li{position:relative; display:inline-block; width:386px; margin:0 10px 15px; text-align:left;}
.box-list li span{position:relative; display:block; width:386px; height:219px;}
.box-list li img{width:100%;height:219px;}
.box-list li strong{display:block; height:38px; line-height:38px; font-size:16px; font-weight:normal; letter-spacing:3px; color:#bbb; }
.box-list.brand-film-list {padding-top:0;}

/* 360vr */
.section.vr {white-space:nowrap; /*min-height:938px; */background-color:#0f1012;}
.section.vr > div:after {content:''; vertical-align:middle; display:inline-block; height:100%;}

.vr .box-title{position:absolute; top:120px; left:50%; width:1000px; margin-left:-500px;}
.vr .box-reel{vertical-align:middle; position:relative; display:inline-block; width:100%; padding-top:120px; text-align:center;}
.vr .box-reel > div:first-of-type{background:url('../images/bg_vr.png') no-repeat 50% 80%}
.vr .box-reel .notice{ height:18px; margin:50px auto 0; text-align:center; color:#a6a6a7;}
.vr .box-reel .notice i{display:inline-block; width:20px; height:20px; margin-right:10px; vertical-align:middle; background:url('../images/bg_notice.png') no-repeat 0 0;}
.vr .reel{width:745px; margin:0 auto;}
.vr .reel img {margin-top:-30px;}

.vr .select-color{position:relative; height:55px; text-align:center; color:#101010;}
.vr .select-color li{display:inline-block; margin:0 4px; }
.vr .select-color li i{display:none; position:absolute; bottom:0; left:0; right:0; font-size:14px; color:#fff; text-align:center;}
.vr .select-color li.active i{display:block;}
.vr .select-color li button span{position:relative; display:inline-block; width:20px; height:20px; font:0/0 a; vertical-align:top; background:url('../images/bg_colorchip.png')}
.vr .select-color li button.white span{background-position:0 0;}
.vr .select-color li button.silver span{background-position:0 -20px;}
.vr .select-color li button.metal span{background-position:0 -40px;}
.vr .select-color li button.blue span{background-position:0 -80px;}
.vr .select-color li button.brown span{background-position:0 -60px;}
.vr .select-color li button.black span{background-position:0 -100px;}
.vr .select-color li.active button span:after{content:''; position:absolute; left:50%; top:50%; display:block; width:6px; height:6px; margin:-3px 0 0 -3px; background:url('../images/bg_color_active.png') no-repeat 0 0;}

.reel-preloader {display:none !important; bottom:130px !important;}
iframe[name='google_conversion_frame'] {position:absolute; bottom:0; left:0; z-index:-1;}

/* whos */
.section.whos{background:#000 url('../images/bg_whos.jpg') no-repeat 50% 50%; background-size:cover; }
.whos .contents{position:relative; margin:0 auto; padding:120px 0 100px; -webkit-box-sizing:border-box; box-sizing:border-box;}
.whos .wrap-video{ width:76.3888%; height:619px; margin:-95px auto 0; background:url('../images/bg_whos_video.jpg'); background-size:cover;}
.share-sns{float:right; width:35px; margin-top:-145px; margin-right:8.6%;}
.share-sns:after{content:''; display:block; clear:both;}
.share-sns a{display:block; width:35px; height:35px; margin-top:10px; font:0/0 a; background:url('../images/icon_sns.png')}
.share-sns .facebook{background-position:0 0;}
.share-sns .kakaostory{background-position:0 -45px;}
.share-sns .youtube{background-position:0 -90px;}
.btn-scroll{display:block; width:58px; margin:50px auto 0; text-align:center; }
.section.usp .btn-top{position:absolute; right:30px; bottom:60px; z-index:10; display:block; }
.section.whos .fp-tableCell {vertical-align:top; padding-top:200px;}


.section.usp{background-color:#0f1012;}
.usp .contents{min-height:900px; padding-top:120px; -webkit-box-sizing:border-box; box-sizing:border-box;}
.slider-usp{margin-top:60px; padding-bottom:65px;}
.slider-usp .item{width:50%;}
.slider-usp .item .txt{display:none;}

.slider-usp .pagination{margin-top:30px; text-align:center;}
.swiper-pagination-bullet{height:0; opacity:1; background:none;}
.swiper-container-horizontal>.swiper-pagination-bullets .swiper-pagination-bullet{position:relative; width:40px; margin:0;}
.swiper-container-horizontal>.swiper-pagination-bullets .swiper-pagination-bullet:before{content:'';position:absolute;left: -20px;right: -20px;height:1px;background-color:#232d31;}
.swiper-container-horizontal>.swiper-pagination-bullets .swiper-pagination-bullet:after{content:''; position:absolute; left:50%; top:50%; display:block; width:5px; height:5px; margin:-2.5px 0 0 -2.5px; background-color:#232d31; -webkit-border-radius:100%; border-radius:100%;}
.swiper-container-horizontal>.swiper-pagination-bullets .swiper-pagination-bullet.swiper-pagination-bullet-active:after{content:''; position:absolute; left:50%; top:50%; width:34px; height:34px; margin:-17px 0 0 -17px; z-index:1; vertical-align:middle; background:url('../images/icon_slider_usp_pagination.png')}

.slider-usp [class^="btn"]{position:absolute; left:50%; bottom:44px; display:inline-block; width:46px; height:46px;}
.slider-usp .btn-prev{margin-left:-205px; background:url('../images/btn_slider_usp_prev.png')}
.slider-usp .btn-next{margin-left:160px; background:url('../images/btn_slider_usp_next.png')}

/* multimedia */
.multimedia{background-color:#0f1012;}
.multimedia > div{white-space:nowrap; position:relative; width:1000px; margin:0 auto;}
.multimedia > div:after {content:''; vertical-align:top; display:inline-block; height:100%;}
.multimedia > div .contents {position:relative; display:inline-block; height:564px; vertical-align:top; margin-top:120px; overflow:hidden;}
*+ html .multimedia > div .contents {display:inline;}

.nav-tabs{position:absolute; right:0; top:0; z-index:3}
.nav-tabs a{display:inline-block; width:110px; height:30px; margin-left:10px; font:0/0 a;  background-color:#131416;}
.nav-tabs a.active{background-color:#141a1e;}
.nav-tabs a:after{content:''; display:inline-block; width:100%; height:9px; margin-top:10px; background:url('../images/tabs_media.png');}
.nav-tabs .tab-film:after{background-position:0 0;}
.nav-tabs .tab-film.active:after{background-position:0 -40px;}
.nav-tabs .tab-gallery:after{background-position:0 -20px;}
.nav-tabs .tab-gallery.active:after{background-position:0 -60px;}

.tab-contents{display:none; width:1020px; margin-left:-10px; margin-right:-10px; margin-top:60px; background-color:#0f1012}
.tab-contents .item{position:relative; float:left; margin:0 10px 20px;}

.film .item{width:320px; height:200px;}
.film .item:hover:before{display:none;}
.film .item:before{content:''; position:absolute; left:0; top:0; display:block; width:100%; height:100%; background-color:rgba(0,0,0,0.7); z-index:1;}

.film .item:hover span{display:none;}
.film .item span{position:absolute; left:0; top:0; bottom:0; right:0; z-index:2; vertical-align:middle; text-align:center;}
.film .item span img{display:inline-block; vertical-align:middle;}
.film .item span:after{content:''; display:inline-block; height:100%; vertical-align:middle;}

.film .item button{opacity:0; transition: opacity 0.2s; background-image:url('data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7');}
.film .item:hover button{opacity:1;}


.gallery{margin-top:74px;}
.gallery .item{width:233px; height:191px; white-space:nowrap; background:#000; border:1px solid #223741;}
.gallery .item:after{content:''; display:inline-block; height:100%; vertical-align:middle;}
.gallery .item img{display:inline-block; max-width:100%; max-height:100%; vertical-align:middle;}
.gallery .item button{position:absolute; left:0; right:0; top:0; bottom:0; width:100%; height:100%; font:0/0 a; background-image:url('data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7');}

.viewer{display:none;}
.viewer.active{display:block;}
.viewer.active .close {z-index:10;}

.slick-dots{text-align:center;}
.slick-dots li{display:inline-block; width:20px; height:20px; background:url('../images/bg_indicator_media.png') no-repeat 0 -20px;}
.slick-dots li.slick-active{background-position:0 0}
.slick-dots li button{width:100%; height:100%; font:0/0 a;}

.viewer{top:70px; white-space:nowrap;}
.viewer:after{content:''; display:inline-block; height:100%; vertical-align:middle;}
.viewer .vertical{position:relative; display:inline-block; width:100%; vertical-align:middle; white-space:normal;}
.viewer .vertical > div{width:1000px; margin:0 auto; text-align:center;}
.viewer .slick-list{width:910px; height:580px; margin:0 auto;}
.viewer .slick-slide{height:580px; white-space:nowrap;}
.viewer .slick-slide:after{content:''; display:inline-block; height:100%; vertical-align:middle;}
.viewer .slick-slide img{display:inline-block; max-width:100%; max-height:100%; vertical-align:middle; }
.viewer .slick-arrow{position:absolute; top:50%; width:33px; height:55px; margin-top:-27.5px; font:0/0 a; background:url('../images/btn_slide_arrow.png')}
.viewer .slick-prev {left:0; background-position:0 -55px;}
.viewer .slick-next {right:0; background-position:0 0;}
.viewer .btn-close{left: 50%;top: 50%;z-index: 10;margin-left: 402px;margin-top: -290px; background-color:rgba(0,0,0,0.6)}

/* brand */
section.brand{min-height:850px; background:#000 url('../images/bg_brand.jpg') no-repeat center bottom; /*background-size:contain*/}
section.brand.section > div:after {display:none;}
.brand .contents > div{position:relative; width:1000px; margin:0 auto; padding-top:200px;}
.brand  h2{margin-bottom:40px;}
.brand p{margin-left:20px;}

/* calculator */
.calculator{height:auto; padding:80px 0 0; background-color:#0f1012;}
.calculator .contents{padding-top:40px; overflow:hidden;}
.calculator .box-title h2{margin-bottom:20px;}

.box-calc{width:1000px; margin:0 auto; }
.box-select{position:relative; width:1000px; height:410px; margin:50px 0 120px; padding-bottom:75px; background:url('../images/bg_box_select.png') no-repeat 0 0;}
.box-select > .notice{margin:10px 0 0 40px;}

.box-select .steps:after{content:''; display:block; clear:both;}
.box-select .steps > li{position:relative; float:left; width:330px; height:360px; margin-left:5px; padding:25px 40px 30px; box-sizing:border-box; -webkit-box-sizing:border-box;}
.box-select .steps > li:first-child{margin-left:0;}
.box-select .steps > li.active{background:url('../images/bg_box_select_step_active.png') no-repeat -5px -5px;}
.box-select .steps > li.dim:after{content:''; position:absolute; left:0; top:0; bottom:0; right:0; display:block; background:rgba(12,13,14,0.5) }
.box-select .steps > li:before {content:''; position:absolute; top:0; left:0; z-index:10; width:100%; height:100%; background-image:url('data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7');}
.box-select .steps > li.complete:before,
.box-select .steps > li.active:before {display:none;}

.box-select .tit{height:35px; color:#232d31; font-size:18px;}
.box-select .active .tit,
.box-select .complete .tit{color:#70d5ff;}
.box-select .open-model-info {font:0/0 a; position:absolute; top:10px; right:-45px; width:45px; height:160px; background-repeat:no-repeat; background-color:#223741; background-position:50% 50%; background-image:url('../images/btn_model_info.png');}
.layer-model-info {display:none; position:fixed; top:50%; left:50%; width:770px; z-index:10; height:412px; margin:-206px 0 0 -385px; padding:40px; box-sizing:border-box; background-color:#0f1012;}
.layer-model-info.active {display:block;}
