.features .bg{position:relative; display:block; min-width:1000px; /*max-width:1440px;*/ margin:0 auto; background-repeat:no-repeat; background-position:50% 50%; }
.exterior .bg{height:820px; background-image:url('../images/bg_exterior.jpg')}
.interior .bg{height:950px; background-image:url('../images/bg_interior.jpg')}
.space .bg{height:600px; background-image:url('../images/bg_space.jpg')}
.fundriving .bg{height:600px; background-image:url('../images/bg_fundriving.jpg')}
.exterior .box-layer,
.interior .box-layer{position:absolute; top:0; left:0; right:0; bottom:0; width:1000px; height:100%; margin:0 auto;}
.exterior .btn-layer,
.interior .btn-layer{position:absolute; }
.exterior .btn-layer.exterior0{left:215px; top:355px;}
.exterior .btn-layer.exterior1{left:452px; top:411px;}
.exterior .btn-layer.exterior2{left:278px; top:498px;}
.exterior .btn-layer.exterior3{left:479px; top:519px;}
.exterior .btn-layer.exterior4{left:779px; top:265px;}
.exterior .btn-layer.exterior5{left:869px; top:234px;}

.interior .btn-layer.interior0{left:20px; top:659px;}
.interior .btn-layer.interior1{left:598px; top:590px;}
.interior .btn-layer.interior2{left:683px; top:272px;}

.features .layer{left:50%; top:185px; width:570px; margin-left:-285px; box-sizing:border-box;}
.box-layer .layer .box-title{position:static; width:470px; margin:0; padding-bottom:0; border:none; background:url('../images/txt_layer_feature.png') }
.box-layer .layer h3,
.box-layer .layer .box-txt{font:0/0 a;}
.box-layer .layer .box-txt{margin-top:24px; background:url('../images/txt_layer_feature.png') }
.box-layer .layer.exterior0 .box-title{background-position:0 0}
.box-layer .layer.exterior1 .box-title{background-position:0 -50px}
.box-layer .layer.exterior2 .box-title{background-position:0 -100px;}
.box-layer .layer.exterior3 .box-title{background-position:0 -150px}
.box-layer .layer.exterior4 .box-title{background-position:0 -200px}
.box-layer .layer.exterior5 .box-title{background-position:0 -250px}

.box-layer .layer.interior0 .box-title{background-position:0 -300px}
.box-layer .layer.interior0 .box-txt{height:80px; background-position:0 -450px;}
.box-layer .layer.interior1 .box-title{background-position:0 -350px}
.box-layer .layer.interior1 .box-txt{height:50px; background-position:0 -600px;}
.box-layer .layer.interior2 .box-title{background-position:0 -400px;}
.box-layer .layer.interior2 .box-txt{height:70px; background-position:0 -650px;}

.bg-top .light {width:10px; height:10px; position:absolute; z-index:19; visibility:visible;}
.bg-top .light.start {visibility:hidden;}
.bg-top .light1 {top:-15px;}
.bg-top .light2 {top:85px;}
.bg-top .light:after {content:''; position:absolute; top:50%; left:50%; display:block; width:218px; height:114px; margin:-57px 0 0 -109px; background:url('../images/light.png');}


/* error */
.error-page {background-color:#050505;}
.error-page #header {top:0;}
.error-page #fullpage {text-align:center; height:790px; background-repeat:no-repeat; background-position:50% 0; background-image:url('../images/error.jpg');}

.mr-niro {padding-top:0;}
.mr-niro #header {top:0;}
.mr-niro .whos .contents {padding-top:200px;}
