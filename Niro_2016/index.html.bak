					if (offset < 0) offset = 1000;
					$path[0].style.strokeDashoffset = offset;
					offset--;

					raf = requestAnimationFrame(function() {
						_this.start();
					});

					return this
				},
				stop:function() {
					cancelRequestAnimationFrame(raf);
				}
			};

			return this.each(function() {
				new Offset(this).init();
			});
		}
	})();

